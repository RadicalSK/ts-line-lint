{
    "name": "ts-line-lint",
    "version": "1.1.0",
    "description": "enforce consistent use of blank lines in TypeScript files",
    "keywords": [
        "lint",
        "typescript",
        "blank",
        "line"
    ],
    "main": "build/index.js",
    "bin": {
        "ts-line-lint": "./build/index.js"
    },
    "scripts": {
        "build": "babel src --out-dir build --ignore '**/*.spec.js'",
        "prepublishOnly": "npm run build",
        "release": "np",
        "start": "node build/index.js",
        "test": "mocha --ui bdd ./src/**/*.spec.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RadovanBednar/ts-line-lint"
    },
    "babel": {
        "presets": [
            "latest"
        ]
    },
    "author": "Radovan Bedn√°r",
    "license": "ISC",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-latest": "^6.24.1",
        "chai": "^4.1.2",
        "mocha": "^5.2.0",
        "mock-fs": "^4.7.0",
        "np": "^3.0.4"
    }
}
